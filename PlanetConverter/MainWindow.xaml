                <StackPanel Orientation="Horizontal">
                    <TextBlock Text="Time :         " Foreground="Black"></TextBlock>
                    <TextBox x:Name="TimeTextBox" Height="23" TextWrapping="Wrap" Text="" Width="280"/>
                </StackPanel>
